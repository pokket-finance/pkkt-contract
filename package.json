    "@openzeppelin/contracts-upgradeable": "4.3.1",
    "@ribbon-finance/rvol": "^1.4.0",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "axios": "^0.21.4",
    "cron": "^1.8.2",
    "defender-admin-client": "^1.12.1",
    "defender-relay-client": "^1.12.1",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "got": "^10.7.0",
    "json-stable-stringify": "^1.0.1",
    "moment-timezone": "^0.5.33",
    "simple-git": "^2.45.1"
  },
  "mocha": {
    "timeout": 20000
  }
