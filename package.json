  "scripts": {
    "start": "ts-node .", 
    "compile": "npx hardhat compile",  
    "deploy": "npx hardhat prepare-accounts && npx hardhat deploy --network bsctest", 
    "config-email": "npx hardhat configure-email", 
    "set-settler": "npx hardhat set-settler --network bsctest", 
    "transfer-ownership": "npx hardhat transfer-ownership --network bsctest", 
    "new-epoch": "npx hardhat prepare-accounts --forcesettlerkey && npx hardhat new-epoch --network bsctest", 
